<data>
<record id="account_asset_change_estimation_salvage_server_action" model="ir.actions.server">
    <field name="name">Asset Salvage Value Estimation Change</field>
    <field name="type">ir.actions.server</field>
    <field name="state">code</field>
    <field name="model_id" ref="fixed_asset_estimation_change.model_account_asset_change_estimation_salvage"/>
